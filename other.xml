<application>
  <component name="PyTestFrameworkService">
    <option name="SDK_TO_PYTEST">
      <map>
        <entry key="$USER_HOME$/.pyenv/shims/python" value="true" />
        <entry key="$USER_HOME$/.pyenv/versions/3.4.3/bin/python3.4" value="true" />
      </map>
    </option>
    <option name="SDK_TO_NOSETEST">
      <map>
        <entry key="$USER_HOME$/.pyenv/shims/python" value="true" />
        <entry key="$USER_HOME$/.pyenv/versions/3.4.3/bin/python3.4" value="false" />
      </map>
    </option>
    <option name="SDK_TO_ATTEST">
      <map>
        <entry key="$USER_HOME$/.pyenv/shims/python" value="false" />
        <entry key="$USER_HOME$/.pyenv/versions/3.4.3/bin/python3.4" value="false" />
      </map>
    </option>
  </component>
</application>